{
	"name": "@yrd/uikit",
	"version": "0.0.1",
	"private": false,
	"homepage": "https://goggle.com",
	"description": "A UI Library for Modern Web Apps, powered by Vue & Tailwind CSS.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nuxt/ui.git"
	},
	"type": "module",
	"author": {
		"name": "Moshkin Denis",
		"email": "denis.moshkin.007@yandex.ru"
	},
	"keywords": [
		"sigma-vue",
		"vue3",
		"tailwindcss",
		"component library",
		"vue",
		"vuejs",
		"vue.js",
		"typescript",
		"vue-components",
		"ui",
		"tailwind",
		"framework",
		"ui-framework"
	],
	"exports": {
		".": {
			"import": "./dist/sigma-vue.es.js",
			"require": "./dist/sigma-vue.umd.js"
		}
	},
	"scripts": {
		"dev": "npm run docs:dev",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"build": "vite build",
		"check": "npm run format && npm run type-check && npm run test:unit && npm run lint",
		"play": "vite dev --base ./playground",
		"preview": "vite preview",
		"test:unit": "vitest run",
		"test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
		"type-check": "vue-tsc --build",
		"lint": "eslint . --fix",
		"format": "prettier --write src/",
		"buildfddf": "run-p type-check \"build-only {@}\" --",
		"prepare": "husky",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview"
	},
	"dependencies": {
		"@tailwindcss/postcss": "^4.1.13",
		"@vue/shared": "^3.5.21",
		"@vueuse/core": "^13.9.0",
		"class-variance-authority": "^0.7.1",
		"dayjs": "^1.11.18",
		"pinia": "^3.0.3",
		"tailwind-variants": "^3.1.1",
		"type-fest": "^4.41.0",
		"vue": "^3.5.21",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@histoire/plugin-vue": "^1.0.0-alpha.3",
		"@iconify/vue": "^5.0.0",
		"@tailwindcss/vite": "^4.1.13",
		"@tsconfig/node22": "^22.0.2",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^24.3.1",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitejs/plugin-vue-jsx": "^5.1.1",
		"@vitest/eslint-plugin": "1.3.9",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^14.6.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.8.1",
		"@whyframe/core": "^0.1.14",
		"@whyframe/vue": "^0.1.9",
		"autoprefixer": "^10.4.21",
		"cypress": "^15.1.0",
		"eslint": "^9.35.0",
		"eslint-plugin-cypress": "^5.1.1",
		"eslint-plugin-perfectionist": "^4.15.0",
		"eslint-plugin-vue": "^10.4.0",
		"histoire": "^1.0.0-alpha.3",
		"husky": "^9.1.7",
		"jiti": "^2.5.1",
		"jsdom": "^26.1.0",
		"npm-run-all2": "^8.0.4",
		"prettier": "^3.6.2",
		"sass-embedded": "^1.92.1",
		"start-server-and-test": "^2.1.0",
		"tailwindcss": "^4.1.13",
		"typescript": "5.9.2",
		"vite": "^7.1.4",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-vue-devtools": "^8.0.1",
		"vitepress": "^2.0.0-alpha.12",
		"vitest": "^3.2.4",
		"vue-tsc": "^3.0.6"
	},
	"license": "MIT"
}
